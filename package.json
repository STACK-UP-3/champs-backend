{
  "name": "barefoot-nomad",
  "version": "1.0.0",
  "description": "app to allow booking and reservation of trips",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha --require @babel/polyfill --require @babel/register server/tests/**/*.js* --recursive --exit",
    "local:coveralls": "nyc npm run test && nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "connectdb": "nodemon --exec babel-node ./server/config/dbconnect.js ",
    "migrate": "sequelize db:migrate & sequelize db:seed:all",
    "serve": "nodemon --exec babel-node server/index.js",
    "start": "babel-node -- server/index.js",
    "lint": "eslint --fix ./server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/STACK-UP-3/champs-backend.git"
  },
  "author": "champs-dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/STACK-UP-3/champs-backend/issues"
  },
  "homepage": "https://github.com/STACK-UP-3/champs-backend#readme",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2",
    "sequelize-cli": "^5.5.1"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "dotenv": "^8.2.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "express": "^4.17.1",
    "logdna-winston": "^2.3.1",
    "winston": "^3.2.1",
    "pg": "^7.18.2",
    "swagger-ui-express": "^4.1.3"
  }
}
