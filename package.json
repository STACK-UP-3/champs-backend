{
  "name": "barefoot-nomad",
  "version": "1.0.0",
  "description": "app to allow booking and reservation of trips",
  "main": "index.js",
  "nyc": {
    "exclude": [
      "server/helpers/loggerHelper.js",
      "server/config/*",
      "server/sequelize/models/*.js"
    ]
  },
  "scripts": {
    "test": "npm run droptables:test && npm run migrate:test && env NODE_ENV=test nyc mocha --require @babel/polyfill --require @babel/register server/tests/*.js* --recursive --timeout 200000 --exit",
    "local:coveralls": "nyc npm run test && nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": " npx sequelize-cli db:seed:all",
    "seed:drop": "npx sequelize-cli db:seed:undo:all",
    "migrate:test": "env NODE_ENV=test sequelize db:migrate && env NODE_ENV=test sequelize db:seed:all",
    "drop": "sequelize db:migrate:undo:all",
    "droptables:test": "env NODE_ENV=test sequelize db:migrate:undo:all",
    "serve": "nodemon --exec babel-node server/index.js",
    "start": "env NODE_ENV=production npm run migrate && env NODE_ENV=production npm run seed:drop && env NODE_ENV=production npm run seed && babel-node ./server/index.js",
    "droptables": "sequelize db:migrate:undo:all",
    "lint": "eslint --fix ./server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/STACK-UP-3/champs-backend.git"
  },
  "author": "champs-dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/STACK-UP-3/champs-backend/issues"
  },
  "homepage": "https://github.com/STACK-UP-3/champs-backend#readme",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.11",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2",
    "sequelize-cli": "^5.5.1"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@hapi/joi": "^17.1.1",
    "@sendgrid/mail": "^6.5.5",
    "bcrypt": "^4.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "logdna-winston": "^2.3.1",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "sendgrid": "^5.2.3",
    "sequelize": "^5.21.5",
    "swagger-ui-express": "^4.1.4",
    "winston": "^3.2.1"
  }
}
